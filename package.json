{
  "name": "hl7-tstd",
  "version": "0.3.1",
  "description": "A simple package to create, parse & transform HL7 message.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "require": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsup"
  },
  "keywords": [
    "hl7",
    "parse",
    "create",
    "transform",
    "format",
    "hl7-tstd"
  ],
  "author": "1nandagopal",
  "license": "Apache-2.0",
  "homepage": "https://github.com/1nandagopal/hl7-tstd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1nandagopal/hl7-tstd.git"
  },
  "bugs": {
    "url": "https://github.com/1nandagopal/hl7-tstd/issues"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
